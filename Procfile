web: java -Dserver.port=$PORT -Dspring.data.mongodb.uri=$MONGO_URI -Dencryption.password=$ENCRYPTION_KEY -Dauth.client.id=$CLIENT_ID -Dauth.client.password=$CLIENT_PASSWORD -Dauth.url.user=$USER_URL -Dauth.url.token=$TOKEN_URL -Dspring.http.multipart.max-file-size=70Mb spring.http.multipart.max-request-size=70Mb -Dacao.origins=http://cantina.voget.io -D $JAVA_OPTS -jar build/libs/*.jar
